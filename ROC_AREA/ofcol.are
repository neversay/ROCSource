>give_prog 9221~
if is_qmark($i) != adv_eq_making
	if is_qmark($n) == adv_do_mission 300016
		if is_qmark($n) !& adv_do_mission 100001
			mpmod_qmark $n adv_do_mission v0 | 1
			mpmod_qmark $n adv_do_mission v1 + 1
