                modhmv $n 03.99 99 0
                modhmv $n 13.99 99 0
                modhmv $n 23.99 99 0
                echoat $n 你覺得全身的能量幾乎都被抽了出去！
                echoat $n 一顆能源球體瞬間在你面前成形！
                oload 1217 1 floor
        endif
endif
~
