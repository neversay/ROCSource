>defun_prog malathar_warrior_five~
if is_qmark($n) == malathar_quest 100031
        mpmod_qmark $n malathar_quest v0 = 32
endif
if is_qmark($n) == malathar_quest 100032
        mpmod_qmark $n malathar_quest v0 = 33
        mpforcer here stun $n 20
        if is_qmark($n) == malathar_quest 200015
                mpadd_qmark self malathar_talking 10 0 0 0 0 1
                tell $n 你還活著真是太好了，敵人這次來得太突然，差點來不及應對。
                mpcallout 1 mpecho $I似乎鬆了一口氣。
                mpcallout 2 tell $n 我得進行準備，敵人應該會再度發動攻擊。
                mpcallout 3 tell $n 你還是先離開吧，下次恐怕就沒有這麼好運了。
                mpcallout 4 wave $n
                mpcallout 4 mpdel_qmark self malathar_talking
        else
                if is_qmark($n) == malathar_quest 200045
                        mpadd_qmark self malathar_talking 10 0 0 0 0 1
                        gasp $n
                        mpcallout 1 tell $n 真、真是令人不敢相信......
                        mpcallout 2 mpecho $I吃驚地看著手上的報告書，似乎看到什麼神奇的東西。
                        mpcallout 3 tell $n 沒想到所有的敵軍竟然被你一個人全滅了，真是可怕啊......
                        mpcallout 4 ponder
                        mpcallout 5 tell $n 那麼我想......你應該有資格接受這個東西吧......
                        mpcallout 6 mpecho $I從桌子的夾層中取出了一枚玉石。
                        mpcallout 7 tell $n 這個東西，我們只頒給在戰場上表現特別英勇的勇者。
                        mpcallout 8 tell $n 現在我認為你有這個資格，請你收下吧！
                        mpcallout 9 mpoload 80183 $n 36 target
                        mpcallout 9 mpresp $n 3
                        mpcallout 9 mpexp $n 10000
                        mpcallout 9 mpskillexp $n 30000
                        mpcallout 9 mpdel_qmark self malathar_talking
                else
                        mpadd_qmark self malathar_talking 10 0 0 0 0 1
                        tsk $n
                        mpcallout 1 tell $n 本來我是要你避難的，想不到你居然也投入戰場了。
                        mpcallout 2 tell $n 而且還取得了相當的戰果，真是不錯。
                        mpcallout 3 hand $n
                        mpcallout 4 tell $n 感謝你的協助。
                        mpcallout 5 mpresp $n 1
                        mpcallout 5 mpexp $n 10000
                        mpcallout 5 mpskillexp $n 17500
                        mpcallout 5 mpdel_qmark self malathar_talking
                endif
        endif
else
        tell $n 歡迎光臨，你有什麼事嗎？
endif
~
>fight_prog 100~
if is_qmark($n) != malathar_enemy
        mpadd_qmark $n malathar_enemy 24 0 0 0 0 1
endif
if rand(25)
        frenzy
        mpnowait
else
        if rand(33)
                change $r
                fulgurant
                mpnowait
                cast paralyze $n
                mpnowait
        else
                if rand(50)
                        circular
                        mpnowait
                        icecore
                        mpnowait
                else
                        mpechoat $n 當你正忙著與$I戰鬥時，一個鬼鬼祟祟的傢伙忽然自背後捅了你一刀！
                        mpforcer here add_affect $n tardy 1040 10 34 150 4096
                endif
        endif
endif
~
>death_prog 100~
mpdis_magic self all
mpat 5 cast 'detect hidden'
mpat 5 cast 'detect invis'
mpat 5 cast infravision
if is_qmark($n) != malathar_enemy
        mpadd_qmark $n malathar_enemy 200 0 0 0 0 1
else
        mpmod_qmark $n malathar_enemy timer + 50
endif
~
