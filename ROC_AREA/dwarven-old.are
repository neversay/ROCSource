		mpcallout 3 mpmod_qmark $n homeport_dwarf_quest v0 = 3
	endif
endif
~
>talk_prog report~
if is_qmark($i) == quest_talking
	break
endif
if is_qmark($n) == homeport_dwarf_quest 100003
	mpadd_qmark self quest_talking 5 0 0 0 0 0
	ponder $n
	mpcallout 1 tell $n 真不知道是否該相信你。
	mpcallout 2 tell $n 不過你是要拿礦石(mineral)還是拿裝甲(armor)回去呢？
	mpcallout 3 tell $n 我想那老闆應該有跟你說清楚吧。
	mpcallout 3 mpdel_qmark self quest_talking
	mpcallout 3 mpmod_qmark $n homeport_dwarf_quest v0 = 5
endif
~
>talk_prog armor~
if is_qmark($i) == quest_talking
	tell $n 我現在在忙，你等等在來吧。
	break
endif
if is_qmark($n) == homeport_dwarf_quest 100005
	mpmod_qmark $n homeport_dwarf_quest v0 = 6
	mpadd_qmark self quest_talking 5 0 0 0 0 0
	flop $n
	mpcallout 1 tell $n 西克索真的這樣跟你說嘛？
	mpcallout 2 mpechoat $n $I 喃喃自語說：可是這明明就該是機密....。
	mpcallout 3 mpechoat $n $I 陷入了思考。
	mpcallout 5 tell $n 好吧，既然西克索都這樣跟你說了，那你稍等一下。
	mpcallout 6 mpechoat $n $I 打開了商店的櫃子，謹慎的拿出了一件護甲。
	mpcallout 7 mpoload 18016 40
	mpcallout 8 give dwarven-homeport $n
	mpcallout 9 tell $n 請務必把這胸甲拿給西克索。
	mpcallout 10 bow $n
	mpcallout 10 mpmod_qmark $n homeport_dwarf_quest v2 = 1
endif
~
>talk_prog mineral~
if is_qmark($n) == homeport_dwarf_quest 100005
	if is_qmark($n) == quest_talking
		tell $n 我現在在忙，你等等再來吧。
